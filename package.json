{
	"name": "Rodan",
	"version": "2.1.2",
	"private": true,
	"author": "Caleb Depatie",
	"description": "Front End interface for SP",
	"main": "electron.js",
	"scripts": {
		"update": "git pull && yarn",
		"dev": "yarn build:dev && yarn start",
		"build:dev": "conc \"yarn build:dev:main\" \"yarn build:dev:renderer\"",
		"build:dev:main": "webpack --config ./webpack.config.main.js --mode=development",
		"build:dev:renderer": "webpack --config ./webpack.config.renderer.dev.js --mode=development",
		"build:prod": "conc \"yarn build:prod:main\" \"yarn build:prod:renderer\"",
		"build:prod:main": "webpack --config ./webpack.config.main.js --mode=production",
		"build:prod:renderer": "webpack --config ./webpack.config.renderer.prod.js --mode=production",
		"package": "yarn build:prod && electron-builder",
		"start": "conc \"yarn start:main\" \"yarn start:renderer\"",
		"start:main": "electron build/electron.js",
		"start:renderer": "webpack serve --config ./webpack.config.renderer.dev.js --mode=development"
	},
	"build": {
		"asar": true,
		"files": [
			"build/*",
			"common/*"
		],
		"extraFiles": [
			"*.env",
			"*.node",
			"*.pdb"
		],
		"win": {
			"icon": "./src/renderer/Public/favicon.ico",
			"target": "nsis"
		},
		"linux": {
			"icon": "./src/static/icon.png",
			"target": "AppImage",
			"category": "Office"
		}
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^6.5.2",
		"@popperjs/core": "^2.11.8",
		"@types/node": "^20.12.7",
		"@types/react": "^18.3.1",
		"@types/react-dom": "^18.2.24",
		"@types/react-router-dom": "^5.3.3",
		"chart.js": "^4.4.2",
		"dayjs": "^1.11.11",
		"dotenv": "^16.4.5",
		"electron-fetch": "^1.9.1",
		"mermaid": "^10.9.0",
		"node-ssh-forward": "^0.7.2",
		"path-browserify": "^1.0.1",
		"primeflex": "^3.3.1",
		"react-chartjs-2": "^5.2.0",
		"react-popper": "^2.3.0",
		"react-remark": "^2.1.0",
		"react-router-dom": "^6.25.1",
		"react-toastify": "^10.0.5",
		"react-transition-group": "^4.4.5",
		"rehype-highlight": "^7.0.0",
		"rehype-mermaid": "^2.1.0",
		"rehype-sanitize": "^6.0.0",
		"remark-gfm": "^4.0.0"
	},
	"peerDependencies": {},
	"devDependencies": {
		"@babel/core": "^7.24.4",
		"@babel/plugin-syntax-flow": "^7.24.1",
		"@babel/plugin-transform-react-jsx": "^7.22.5",
		"@babel/plugin-transform-runtime": "^7.24.3",
		"@babel/preset-react": "^7.24.1",
		"@babel/preset-typescript": "^7.24.1",
		"@webpack-cli/serve": "^1.7.0",
		"concurrently": "^8.2.2",
		"copy-webpack-plugin": "^12.0.2",
		"css-loader": "^7.1.0",
		"electron": "^31.3.1",
		"electron-builder": "^24.13.3",
		"electron-webpack": "^2.8.2",
		"electron-webpack-ts": "^4.0.1",
		"html-webpack-plugin": "^5.6.0",
		"loader-utils": "^3.2.1",
		"react": "^18.3.1",
		"react-dom": "^18.0.0",
		"react-scripts": "^5.0.1",
		"sass": "^1.74.1",
		"sass-loader": "^14.1.1",
		"source-map-support": "^0.5.21",
		"style-loader": "^4.0.0",
		"style-resources-loader": "^1.4.1",
		"terser-webpack-plugin": "^5.3.6",
		"ts-loader": "~9.5.1",
		"typescript": "^5.4.4",
		"webpack": "^5.91.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.0.0",
		"webpack-merge": "^5.10.0"
	}
}
